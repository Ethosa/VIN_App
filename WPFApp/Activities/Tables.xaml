<Window x:Class="WPFApp.Activities.Tables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApp.Activities"
        mc:Ignorable="d"
        Title="Tables" Height="720" Width="1280">
    <Grid>
        <DataGrid x:Name="gibddDataGrid" HorizontalAlignment="Left" Height="689" VerticalAlignment="Top" Width="1042">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Фамилия"/>
                <DataGridTextColumn Header="Имя"/>
                <DataGridTextColumn Header="Отчество"/>
                <DataGridTextColumn Header="Серия паспорта"/>
                <DataGridTextColumn Header="Номер паспорта"/>
                <DataGridTextColumn Header="Почтовый индекс"/>
                <DataGridTextColumn Header="Адрес прописки"/>
                <DataGridTextColumn Header="Фактический адрес"/>
                <DataGridTextColumn Header="Компания"/>
                <DataGridTextColumn Header="Должность"/>
                <DataGridTextColumn Header="Телефон"/>
                <DataGridTextColumn Header="E-mail"/>
                <DataGridTextColumn Header="Фото"/>
            </DataGrid.Columns>
        </DataGrid>


    </Grid>
</Window>
